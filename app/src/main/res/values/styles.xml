<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/full_transparent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="CustomView">
        <attr name="fontcustom" format="string" />

    </declare-styleable>

    <style name="mySpinnerItemStyle" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:textColor">@color/dark_grey</item>
        <item name="android:textSize">@dimen/_11sdp</item>
    </style>

    <style name="MySeekBar" parent="Widget.AppCompat.SeekBar">
     <!--   <item name="android:progressBackgroundTint">#f4511e</item>
        <item name="android:progressTint">#388e3c</item>-->
        <item name="android:thumbTint">@color/black</item>
    </style>

</resources>